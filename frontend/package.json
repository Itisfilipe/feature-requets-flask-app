  "dependencies": {
    "bootstrap": "3.3.7",
    "jquery": "^3.2.1",
    "knockout": "3.4.2",
    "sammy": "0.7.6"
  },
  "devDependencies": {
    "css-loader": "^0.28.4",
    "replace-in-file": "^2.5.0",
